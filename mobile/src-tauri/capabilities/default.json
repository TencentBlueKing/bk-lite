{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "store:allow-load",
    "store:allow-get",
    "store:allow-set",
    "store:allow-delete",
    "store:allow-save",
    "store:allow-clear"
  ]
}
