{
  "object_name": "Oracle",
  "instance_type": "oracle",
  "collect_type": "exporter",
  "config_type": ["oracle"],
  "collector": "Oracle-Exporter",
  "instance_id": "{{cloud_region}}_{{ENV_HOST}}_{{ENV_PORT}}",
  "form_fields": [
    {
      "name": "ENV_USER",
      "label": "用户名",
      "type": "input",
      "required": true,
      "description": "用于访问接口的用户名",
      "widget_props": {
        "placeholder": "用户名"
      },
      "data_transform": {
        "origin_path": "base.env_config.USER"
      }
    },
    {
      "name": "ENV_PASSWORD",
      "label": "密码",
      "type": "password",
      "required": true,
      "description": "对应的登录密码",
      "widget_props": {
        "placeholder": "密码"
      },
      "data_transform": {
        "origin_path": "base.env_config.PASSWORD"
      }
    },
    {
      "name": "ENV_SERVICE_NAME",
      "label": "服务名称",
      "type": "input",
      "required": true,
      "description": "要监控的服务名称，确保其与实际运行的服务一致",
      "widget_props": {
        "placeholder": "服务名称"
      },
      "data_transform": {
        "origin_path": "base.env_config.SERVICE_NAME"
      }
    },
    {
      "name": "ENV_LISTEN_PORT",
      "label": "监听端口",
      "type": "inputNumber",
      "required": true,
      "visible_in": "edit",
      "description": "指定程序运行时应监听的端口号，用于接受外部请求和连接",
      "widget_props": {
        "min": 1,
        "precision": 0,
        "placeholder": "监听端口"
      },
      "data_transform": {
        "origin_path": "base.env_config.LISTEN_PORT"
      }
    },
    {
      "name": "ENV_HOST",
      "label": "主机",
      "type": "input",
      "required": true,
      "visible_in": "edit",
      "description": "与设备进行通信的主机",
      "widget_props": {
        "placeholder": "主机地址"
      },
      "data_transform": {
        "origin_path": "base.env_config.HOST"
      }
    },
    {
      "name": "ENV_PORT",
      "label": "端口",
      "type": "inputNumber",
      "required": true,
      "visible_in": "edit",
      "description": "与设备进行通信的端口",
      "widget_props": {
        "min": 1,
        "precision": 0,
        "placeholder": "端口"
      },
      "data_transform": {
        "origin_path": "base.env_config.PORT"
      }
    },
    {
      "name": "interval",
      "label": "间隔",
      "type": "inputNumber",
      "required": true,
      "default_value": 10,
      "description": "监控数据的采集时间间隔（单位：秒）",
      "widget_props": {
        "min": 1,
        "precision": 0,
        "placeholder": "间隔",
        "addonAfter": "s"
      },
      "data_transform": {
        "origin_path": "child.content.config.interval",
        "to_form": {
          "regex": "^(\\d+)s$"
        },
        "to_api": {
          "suffix": "s"
        }
      }
    }
  ],
  "table_columns": [
    {
      "name": "node_ids",
      "label": "节点",
      "type": "select",
      "required": true,
      "widget_props": {
        "placeholder": "请选择节点"
      },
      "enable_row_filter": true
    },
    {
      "name": "ENV_LISTEN_PORT",
      "label": "监听端口",
      "type": "inputNumber",
      "required": true,
      "widget_props": {
        "min": 1,
        "precision": 0,
        "placeholder": "监听端口"
      }
    },
    {
      "name": "ENV_HOST",
      "label": "主机",
      "type": "input",
      "required": true,
      "widget_props": {
        "placeholder": "主机地址"
      },
      "change_handler": {
        "type": "combine",
        "source_fields": ["ENV_HOST", "ENV_PORT"],
        "target_field": "instance_name",
        "separator": ":"
      }
    },
    {
      "name": "ENV_PORT",
      "label": "端口",
      "type": "inputNumber",
      "required": true,
      "widget_props": {
        "min": 1,
        "precision": 0,
        "placeholder": "端口"
      },
      "change_handler": {
        "type": "combine",
        "source_fields": ["ENV_HOST", "ENV_PORT"],
        "target_field": "instance_name",
        "separator": ":"
      }
    },
    {
      "name": "instance_name",
      "label": "实例名称",
      "type": "input",
      "required": true,
      "widget_props": {
        "placeholder": "实例名称",
        "disabled": false
      }
    },
    {
      "name": "group_ids",
      "label": "组",
      "type": "group_select",
      "required": false,
      "widget_props": {
        "placeholder": "请选择组"
      }
    }
  ]
}
