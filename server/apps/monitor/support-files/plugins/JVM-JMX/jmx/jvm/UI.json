{
  "object_name": "JVM",
  "instance_type": "jvm",
  "collect_type": "jmx",
  "config_type": ["jvm"],
  "collector": "JVM-JMX",
  "instance_id": "${cloud_region}_jvm_${jmx_url}",
  "form_fields": [
    {
      "name": "monitor_url",
      "label": "JmxUrl",
      "type": "input",
      "required": true,
      "disabled_in_edit": true,
      "placeholder": "",
      "description": "监控地址说明",
      "locale_key": "monitor.integrations.urlDes"
    },
    {
      "name": "username",
      "label": "用户名",
      "type": "input",
      "required": false,
      "disabled_in_edit": true,
      "locale_key": "monitor.integrations.username"
    },
    {
      "name": "password",
      "label": "密码",
      "type": "password",
      "required": false,
      "disabled_in_edit": true,
      "locale_key": "monitor.integrations.password"
    },
    {
      "name": "LISTEN_PORT",
      "label": "监听端口",
      "type": "inputNumber",
      "required": false,
      "min": 1,
      "precision": 0,
      "description": "监听端口说明",
      "locale_key": "monitor.integrations.listeningPort",
      "description_locale_key": "monitor.integrations.listeningPortDes",
      "data_transform": {
        "origin_path": "base.env_config.LISTEN_PORT",
        "to_form": {
          "type": "number"
        },
        "to_api": {
          "type": "string"
        }
      }
    }
  ],
  "table_columns": [
    {
      "name": "node_ids",
      "label": "节点",
      "type": "node_selector",
      "required": true,
      "locale_key": "monitor.integrations.nodes"
    },
    {
      "name": "jmx_url",
      "label": "URL",
      "type": "input",
      "required": true,
      "locale_key": "monitor.integrations.url",
      "change_handler": {
        "type": "simple",
        "target": "instance_name"
      }
    },
    {
      "name": "ENV_LISTEN_PORT",
      "label": "监听端口",
      "type": "inputNumber",
      "required": false,
      "min": 1,
      "precision": 0,
      "locale_key": "monitor.integrations.listeningPort"
    },
    {
      "name": "instance_name",
      "label": "实例名称",
      "type": "input",
      "required": true,
      "locale_key": "monitor.integrations.instanceName"
    },
    {
      "name": "group_ids",
      "label": "分组",
      "type": "group_selector",
      "required": false,
      "locale_key": "monitor.integrations.groups"
    }
  ],
  "extra_fields": {}
}
