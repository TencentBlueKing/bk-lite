{
  "object_name": "Minio",
  "instance_type": "minio",
  "collect_type": "bkpull",
  "config_type": ["minio"],
  "collector": "Telegraf",
  "instance_id": "${cloud_region}_${host}_${port}",
  "form_fields": [
    {
      "name": "host",
      "label": "主机",
      "type": "input",
      "required": true,
      "disabled_in_edit": true,
      "placeholder": "",
      "description": "主机地址",
      "locale_key": "monitor.integrations.host",
      "description_locale_key": "monitor.integrations.commonHostDes"
    },
    {
      "name": "port",
      "label": "端口",
      "type": "inputNumber",
      "required": true,
      "min": 1,
      "precision": 0,
      "disabled_in_edit": true,
      "description": "端口说明",
      "locale_key": "monitor.integrations.port",
      "description_locale_key": "monitor.integrations.commonPortDes"
    }
  ],
  "table_columns": [
    {
      "name": "node_ids",
      "label": "节点",
      "type": "node_selector",
      "required": true,
      "locale_key": "monitor.integrations.nodes"
    },
    {
      "name": "host",
      "label": "主机",
      "type": "input",
      "required": true,
      "locale_key": "monitor.integrations.host",
      "change_handler": {
        "type": "combine",
        "target": "instance_name",
        "fields": ["host", "port"],
        "separator": ":"
      }
    },
    {
      "name": "port",
      "label": "端口",
      "type": "inputNumber",
      "required": true,
      "min": 1,
      "precision": 0,
      "locale_key": "monitor.integrations.port",
      "change_handler": {
        "type": "combine",
        "target": "instance_name",
        "fields": ["host", "port"],
        "separator": ":"
      }
    },
    {
      "name": "instance_name",
      "label": "实例名称",
      "type": "input",
      "required": true,
      "locale_key": "monitor.integrations.instanceName"
    },
    {
      "name": "group_ids",
      "label": "分组",
      "type": "group_selector",
      "required": false,
      "locale_key": "monitor.integrations.groups"
    }
  ],
  "extra_fields": {}
}
