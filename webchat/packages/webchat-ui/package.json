{
  "name": "@webchat/ui",
  "version": "1.0.0",
  "description": "WebChat UI React components library",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "build:browser": "vite build --config vite.config.umd.ts",
    "dev": "vite",
    "clean": "rm -rf dist",
    "preview": "vite preview",
    "prepublish": "npm run build"
  },
  "keywords": [
    "webchat",
    "react",
    "chat",
    "ui",
    "component"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@ag-ui/core": "^0.0.41",
    "@webchat/core": "file:../webchat-core",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@ant-design/x": "^1.6.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "antd": "^5.28.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "3.4.18",
    "terser": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
